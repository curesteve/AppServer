<meta name="viewport" content="width=device-width,height=device-height,inital-scale=1.0,maximum-scale=1.0,user-scalable=no;">
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="format-detection" content="telephone=no">
<form id="form1" name="form1" method="post" action="">
<table width="600" border="0" cellpadding="0" cellspacing="0" style="margin:100px auto;">
    <tr>
        <td></td>
        <td><span class="message"><%-message%></span></td>
    </tr>
    <%if (result.userId == null){%>
    <tr>
        <td style="width:80px; height:30px;">帐号：</td>
        <td><input type="text" name="name" id="name" value="" /></td>
    </tr>
    <tr>
        <td style="width:80px; height:30px;">密码：</td>
        <td><input type="password" name="password" id="password" value="" /></td>
    </tr>
    <tr>
        <td><input type="hidden" name="opencode" id="opencode" value="<%-usercode%>" /></td>
        <td><input type="submit" name="submit" id="submit" value="登 录" /></td>
    </tr>
    <%} else
    {%>
    <tr>
        <td style="width:80px; height:30px;">密码：</td>
        <td>Hello <%-result.userId%>!</td>
        <td><a href="/WXUser/del?openid=<%-result.openId%>&userid=<%-result.userId%>">解绑帐号</a></td>
    </tr>
    <%}%>
</table>
</form>